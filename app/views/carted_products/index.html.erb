<h1>Items Currently In Your Cart:</h1>
<% @carted_products.each do |carted_product| %>
  <h2><%= carted_product.product.album %></h2>
  <h2><%= number_to_currency(carted_product.product.price) %></h2>
 <% end %> 

<%= form_tag '/orders', method: :post do %>
<br>
  <%= submit_tag "Buy Now" %>
<% end %>
